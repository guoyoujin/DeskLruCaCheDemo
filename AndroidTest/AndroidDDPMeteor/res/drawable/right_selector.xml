<?xml version="1.0" encoding="UTF-8"?>
<!-- 箭头右点击效果 -->
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:drawable="@drawable/right2" />
	<item android:state_selected="true" android:drawable="@drawable/right2" />
	<item android:state_pressed="true" android:drawable="@drawable/right2" />
	<item android:state_checked="true" android:drawable="@drawable/right2"/>
	<item android:state_activated="true" android:drawable="@drawable/right2"/>
	<item android:drawable="@drawable/right"/>
</selector>